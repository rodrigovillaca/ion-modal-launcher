{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ion-modal-launcher": {
      "projectType": "library",
      "root": "projects/ion-modal-launcher",
      "sourceRoot": "projects/ion-modal-launcher/src",
      "prefix": "",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ion-modal-launcher/tsconfig.lib.json",
            "project": "projects/ion-modal-launcher/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ion-modal-launcher/src/test.ts",
            "tsConfig": "projects/ion-modal-launcher/tsconfig.spec.json",
            "karmaConfig": "projects/ion-modal-launcher/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["projects/ion-modal-launcher/tsconfig.lib.json", "projects/ion-modal-launcher/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "ion-modal-launcher"
}
